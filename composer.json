{
    "name": "zcastle/tsclib-php",
    "description": "Libreria para etiquetadora TSC",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jimbert Castillo",
            "email": "jzcastle@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
